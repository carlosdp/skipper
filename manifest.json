{
  "manifest_version": 2,
  "name": "Skipper",
  "version": "1.0.1",
  "description": "Skip through verbose, single-branch directory structures in GitHub.",
  "icons":{
    "16":"skipper16.png",
    "64":"skipper64.png",
    "128":"skipper128.png"
  },
  "content_scripts":[
      {
        "matches":["<all_urls>"],
        "js":["jquery.js", "skipper.js"],
        "css":["skipper.css"]
      }
    ]
}
